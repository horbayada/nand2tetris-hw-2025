// Mux:
// if (sel == 0) out = a
// if (sel == 1) out = b
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
      Not(a=sel, out=nsel);

       And(a=a, b=nsel, out=and0);

      And(a=b, b=sel, out=and1);


    Or(a=and0, b=and1, out=out);
}
