// Mux16:
// 2-input multiplexer with 16-bit wide inputs and output.
// If sel == 0, out = a
// If sel == 1, out = b
CHIP Mux16 {
    IN a[16], b[16], sel;
    OUT out[16];

    PARTS:
       Mux(a=a, b=b, sel=sel, out=out);
}
