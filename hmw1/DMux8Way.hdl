CHIP DMux8Way {
    IN in, sel[3];
    OUT out0, out1, out2, out3, out4, out5, out6, out7;

    PARTS:
    DMux(in=in, sel=sel[0], out0=o0, out1=o1);
    DMux(in=o0, sel=sel[1], out0=o00, out1=o01);
    DMux(in=o1, sel=sel[1], out0=o10, out1=o11);
    DMux(in=o00, sel=sel[2], out0=out0, out1=out1);
    DMux(in=o01, sel=sel[2], out0=out2, out1=out3);
    DMux(in=o10, sel=sel[2], out0=out4, out1=out5);
    DMux(in=o11, sel=sel[2], out0=out6, out1=out7);
}
