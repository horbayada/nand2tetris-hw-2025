/**
 * Demultiplexer:
 * if (sel == 0) out0 = in, out1 = 0
 * if (sel == 1) out0 = 0, out1 = in
 */
CHIP DMux {
    IN in, sel;
    OUT out0, out1;

    PARTS:
       Not(a=sel, out=nsel);           
    And(a=in, b=nsel, out=out0);   
    And(a=in, b=sel, out=out1);    
}
