// File name: Or.hdl

// Or gate:
// If a == 0 and b == 0, out = 0
// If a == 0 and b == 1, out = 1
// If a == 1 and b == 0, out = 1
// If a == 1 and b == 1, out = 1
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
  
    Nand(a=a, b=a, out=n1);
    Nand(a=b, b=b, out=n2);
    Nand(a=n1, b=n2, out=out);
}
